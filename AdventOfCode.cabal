cabal-version:      3.0
name:               AdventOfCode
version:            0.1.0.0
license:            NONE
author:             Jianbo Liu
maintainer:         liuchienpo@gmail.com
build-type:         Simple
extra-doc-files:    CHANGELOG.md
common warnings
    ghc-options: -Wall

library
    import:           warnings
    exposed-modules:  Utils
    build-depends:    base >= 4.18
    hs-source-dirs:   src
    default-language: Haskell2010
    ghc-options: -O2


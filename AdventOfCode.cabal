    import:           warnings
    main-is:          Download.hs
    build-depends:
        base >= 4.18,
        http-conduit,
        text,
        bytestring,
        directory,
        containers,
        http-client,
        AdventOfCode,
    hs-source-dirs:   app
    default-language: Haskell2010
    ghc-options: -O2

